<abr>
    <class>javaClassName</class>
    <code>abrAttributeCode</code>
    <entity type="entityType" id="entityID">entityDescription</entity>
    <revision>abrRevision</revision>
    <job-id>jobID</job-id>
    <timestamp>currentTime</timestamp>
    <user-token>emailAddress</user-token>
    <workgroup id="workgroupID">workgroupName</workgroup>
    <language>
        <read id="nlsID">readLanguageDescription</read>
        <write id="nlsID">writeLanguageDescription</write>
    </language>
    <enterprise>enterprise</enterprise>
    <role code="roleCode">roleDescription</role>
    <dgsubmit>dgSubmitString</dgsubmit>
</abr>
